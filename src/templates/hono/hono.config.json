{
  "name": "Hono REST API",
  "description": "A REST API template using Hono framework with validation support",
  "version": "1.0.0",
  "author": "DuckyCoding",
  "tags": ["api", "rest", "hono", "typescript"],
  "validatorSupport": ["none", "zod", "typebox"],
  "filename": "hono",
  "files": [
    {
      "defaultPath": "src",
      "outputName": "{{entity}}.ts",
      "templatePath": "hono.ts"
    },
    {
      "defaultPath": "src/types",
      "outputName": "{{entity}}.ts",
      "templatePath": "hono.types.ts"
    }
  ],
  "placeholders": [
    {
      "entity": {
        "description": "The entity name",
        "required": true
      }
    },
    {
      "Entity": {
        "description": "The entity name PascalCase",
        "required": true
      }
    },
    {
      "entities": {
        "description": "The entity name as plural",
        "required": true
      }
    }
  ]
}
